{
  "name": "quickgig-frontend",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "ci:verify": "node scripts/ci-verify.mjs",
    "prebuild": "node ./scripts/check-dynamic-route-conflicts.mjs && node ./scripts/assert-valid-revalidate.mjs",
    "build": "node scripts/check-required-env.mjs && next build",
    "start": "next start -p 3000",
    "start:e2e": "next build && next start -p 3000",
    "start:preview": "next start -p 3000",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "lint": "next lint",
    "build:prod": "next build",
    "start:prod": "next start -p 3000",
    "analyze": "ANALYZE=true next build",
    "build:profile": "NODE_OPTIONS='--trace-deopt --trace_gc' next build",
    "seed": "node -r dotenv/config scripts/seed.mjs",
    "db:seed:ph": "tsx scripts/seed-ph-locations.ts",
    "db:seed:test": "node scripts/seedTestData.js",
    "ci:lock": "node scripts/check-lock-sync.mjs",
    "ci:install": "npm ci --no-audit --fund=false --loglevel=error",
    "ci:seed": "node scripts/ci-seed.mjs",
    "autofix:ci": "tsx scripts/autofix.ts",
    "codex:pr": "codex run -p ./codex/tasks/credits_gate.md",
    "codex:e2e": "codex run -p ./codex/tasks/e2e_reliability.md",
    "codex:billing": "codex run -p ./codex/tasks/manual_gcash.md",
    "codex:locations": "codex run -p ./codex/tasks/locations_cascade.md",
    "codex:review": "codex review --config ./codex/config.json",
    "baseline:backfill": "node scripts/baseline/backfill.js",
    "typecheck:scripts": "tsc -p scripts/tsconfig.json",
    "seed:demo": "tsx scripts/seed-demo.ts",
    "playwright:install": "npx playwright install --with-deps",
    "e2e": "playwright test",
    "e2e:headed": "playwright test tests/e2e --headed",
    "e2e:report": "playwright show-report",
    "e2e:seed": "node -e \"console.log('E2E CI: skip seeding')\"",
    "e2e:cleanup": "node -e \"console.log('E2E CI: skip cleanup')\"",
    "e2e:seed:local": "tsx scripts/e2e/seed.ts",
    "e2e:cleanup:local": "tsx scripts/e2e/cleanup.ts",
    "smoke:prod": "BASE_URL=https://app.quickgig.ph npx playwright test -c playwright.smoke.ts",
    "e2e:prod": "BASE_URL=https://app.quickgig.ph npx playwright test",
    "test": "echo \"E2E disabled while product is WIP\" && exit 0",
    "no-legacy": "bash scripts/no-legacy.sh"
  },
  "dependencies": {
    "@jobuntux/psgc": "^0.2.1",
    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "@supabase/auth-helpers-shared": "^0.7.0",
    "@supabase/supabase-js": "^2.45.0",
    "@supabase/ssr": "^0.5.0",
    "autoprefixer": "^10",
    "cookie": "^0.6.0",
    "nanoid": "^3.3.11",
    "next": "^14 || ^15",
    "nodemailer": "^6.9.8",
    "raw-body": "^2.5.2",
    "react": "^18",
    "react-dom": "^18",
    "resend": "^1.0.0",
    "set-cookie-parser": "^2.6.0",
    "stripe": "^16.0.0",
    "swr": "^2.2.5",
    "zod": "^3"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^15.5.0",
    "@playwright/test": "^1.48.0",
    "@types/cookie": "^0.6.0",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/parser": "^6.21.0",
    "autoprefixer": "^10",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.3",
    "fast-glob": "^3.3.2",
    "postcss": "^8",
    "start-server-and-test": "^2.0.0",
    "tailwindcss": "^3.4.1",
    "ts-morph": "^24.0.0",
    "globby": "^14.0.2",
    "tsx": "^4.19.0",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=20 <21"
  },
  "packageManager": "npm@10"
}
